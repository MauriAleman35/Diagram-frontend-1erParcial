{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist",
        "buildCommand": "pnpm build"
      }
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },

  "rewrites": [{ "source": "/(.*)", "destination": "/" }]
}
